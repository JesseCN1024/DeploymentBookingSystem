{
    "ServiceEndpoints": {
        "UserService": {
            "BaseUrl": "http://user-service:80",
            "Endpoints": {
                "Login": "/users/login",
                "GetUser": "/users/{id}"
            }
        },
        "EnvironmentService": {
            "BaseUrl": "http://environment-service:80",
            "Endpoints": {
                "GetEnv": "/environments/{id}"
            }
        },
        "BookingService": {
            "BaseUrl": "http://booking-service:80",
            "Endpoints": {
                "Create": "/booking",
                "GetAll": "/booking"
            }
        }
    }
}
